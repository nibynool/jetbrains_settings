      <mapping pattern="test" type="PLAIN_TEXT" />
      <mapping pattern=".gitkeep" type="PLAIN_TEXT" />
      <mapping pattern="LICENSE" type="PLAIN_TEXT" />
      <mapping ext="config" type="PLAIN_TEXT" />
      <mapping pattern="*.xml.dist" type="XML" />
    </extensionMap>
  </component>
</application>